

<div class="card mt-2">
    <div class="card-header bg-dark text-white">
        <h3> <i class="far fa-images mr-3"></i>Subidas recientes</h3>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each images}}
            <div class="col-md-12 mb-5 align-items-center ">
                <a href="/images/{{uniqueId}}">
                    <img src="/public/upload/{{filename}}" class="w-100 h-100 img-thumbnail" id="img-prueba">
                </a>
            </div>
            {{/each}}
        </div>
    </div>
</div>

